<h1>Current Users</h1>

<table>
  <tr>
    <th>User name</th>
    <th>Email</th>
    <th>Password</th>
 </tr>

<% for user in @users %>
  <tr>
    <td valign="top"><%=h user.screen_name %></td>
    <td valign="top"><%=h user.email %></td>
    <td valign="top">******</td>
    
   </tr>
<% end %>
</table>

